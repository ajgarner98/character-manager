<div class="container">
  <div class="row">
    <div class="col-md-5">
      <button type="button" class="btn btn-secondary" data-bs-toggle="modal" data-bs-target="#skillModal">
        Strength
      </button>
      <%= link_to "Strength", character_armors_path(@character), class: "btn btn-secondary" %>

      <%= link_to "Dexterity", character_armors_path(@character), class: "btn btn-secondary" %>

      <%= link_to "Constitution", character_armors_path(@character), class: "btn btn-secondary" %>
    </div>
    <div class="col">

      <%= link_to "Intelligence", character_armors_path(@character), class: "btn btn-secondary" %>

      <%= link_to "Wisdom", character_armors_path(@character), class: "btn btn-secondary" %>

      <%= link_to "Charisma", character_armors_path(@character), class: "btn btn-secondary" %>
    </div>
  </div>
</div>
